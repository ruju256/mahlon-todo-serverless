{
    "$schema": "http://json-schema.org/draft-07/schema",
    "title": "todo",
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "pattern": "^(?!\\s*$).+"
      },
      "dueDate": {
        "type": "string",
        "pattern": "^(?!\\s*$).+"
      },
      "done": {
        "type": "boolean"
      },
      "attachmentUrl": {
        "type": "string"
      }
    },
    "required": ["name", "dueDate", "done"],
    "additionalProperties": false
  }
  